            @Html.DisplayFor(modelItem => item.Custom2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Custom3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Custom4)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Custom5)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InvoiceReferenceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InvoiceYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InvoiceMonth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InvoiceDate)
        </td>
    </tr>
}

</table>
