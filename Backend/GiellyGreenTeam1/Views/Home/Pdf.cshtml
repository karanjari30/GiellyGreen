@model IEnumerable<DataAccessLayer.Model.Invoice>

@{
    ViewBag.Title = "Pdf";
}

<h2>Pdf</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Custom1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Custom2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Custom3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Custom4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Custom5)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InvoiceReferenceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InvoiceYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InvoiceMonth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InvoiceDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
